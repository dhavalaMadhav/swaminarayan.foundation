<script>
    // Internships and Testimonials Carousel
    document.addEventListener('DOMContentLoaded', function() {
        // ============================================
        // INTERNSHIPS CAROUSEL
        // ============================================
        const internshipTrack = document.querySelector('.internships-track');
        const internshipCards = document.querySelectorAll('.internship-card');
        const internshipPrevBtn = document.querySelector('.internship-btn.prev');
        const internshipNextBtn = document.querySelector('.internship-btn.next');
        const internshipDotsContainer = document.getElementById('internshipDots');
        
        let internshipIndex = 0;
        const internshipTotal = internshipCards.length;
        let internshipAutoSlide;
        let isHoveringInternship = false;
        
        // Create dots for internships
        if (internshipDotsContainer && internshipTotal > 0) {
            for (let i = 0; i < internshipTotal; i++) {
                const dot = document.createElement('button');
                dot.className = 'carousel-dot';
                if (i === 0) dot.classList.add('active');
                dot.addEventListener('click', () => goToInternshipSlide(i));
                internshipDotsContainer.appendChild(dot);
            }
        }
        
        const internshipDots = document.querySelectorAll('#internshipDots .carousel-dot');
        
        function updateInternshipCarousel() {
            if (!internshipCards.length) return;
            
            // Calculate the width of a card plus gap
            const cardWidth = internshipCards[0].offsetWidth + 30; // 30px gap
            
            // Move track to show current card at center
            internshipTrack.style.transform = `translateX(calc(50% - ${(internshipIndex * cardWidth) + (cardWidth / 2)}px))`;
            
            // Update active classes
            internshipCards.forEach((card, index) => {
                card.classList.remove('active');
                if (index === internshipIndex) {
                    card.classList.add('active');
                }
            });
            
            // Update dots
            if (internshipDots.length) {
                internshipDots.forEach((dot, index) => {
                    dot.classList.toggle('active', index === internshipIndex);
                });
            }
        }
        
        function goToInternshipSlide(index) {
            internshipIndex = index;
            updateInternshipCarousel();
            resetInternshipAutoSlide();
        }
        
        function nextInternshipSlide() {
            internshipIndex = (internshipIndex + 1) % internshipTotal;
            updateInternshipCarousel();
        }
        
        function prevInternshipSlide() {
            internshipIndex = (internshipIndex - 1 + internshipTotal) % internshipTotal;
            updateInternshipCarousel();
        }
        
        function startInternshipAutoSlide() {
            if (!isHoveringInternship) {
                internshipAutoSlide = setInterval(nextInternshipSlide, 4000);
            }
        }
        
        function resetInternshipAutoSlide() {
            clearInterval(internshipAutoSlide);
            startInternshipAutoSlide();
        }
        
        // Event listeners for internships
        if (internshipPrevBtn) {
            internshipPrevBtn.addEventListener('click', () => {
                prevInternshipSlide();
                resetInternshipAutoSlide();
            });
        }
        
        if (internshipNextBtn) {
            internshipNextBtn.addEventListener('click', () => {
                nextInternshipSlide();
                resetInternshipAutoSlide();
            });
        }
        
        // Pause on internship card hover
        internshipCards.forEach((card, index) => {
            card.addEventListener('mouseenter', () => {
                isHoveringInternship = true;
                clearInterval(internshipAutoSlide);
            });
            
            card.addEventListener('mouseleave', () => {
                isHoveringInternship = false;
                setTimeout(() => {
                    if (!isHoveringInternship) {
                        startInternshipAutoSlide();
                    }
                }, 100);
            });
        });
        
        // Pause on carousel hover
        const internshipCarousel = document.querySelector('.internships-carousel');
        if (internshipCarousel) {
            internshipCarousel.addEventListener('mouseenter', () => {
                clearInterval(internshipAutoSlide);
            });
            
            internshipCarousel.addEventListener('mouseleave', () => {
                if (!isHoveringInternship) {
                    startInternshipAutoSlide();
                }
            });
        }
        
        // ============================================
        // TESTIMONIALS CAROUSEL
        // ============================================
        const testimonialTrack = document.querySelector('.testimonials-track');
        const testimonialCards = document.querySelectorAll('.testimonial-card');
        const testimonialPrevBtn = document.querySelector('.testimonials-btn.prev');
        const testimonialNextBtn = document.querySelector('.testimonials-btn.next');
        const testimonialCarousel = document.querySelector('.testimonials-carousel');
        
        let testimonialIndex = 0;
        const testimonialTotal = testimonialCards.length;
        let testimonialAutoSlide;
        let isHoveringTestimonial = false;
        
        function updateTestimonialCarousel() {
            if (!testimonialCards.length) return;
            
            // Calculate the width of a card plus gap
            const cardWidth = testimonialCards[0].offsetWidth + 40; // 40px gap
            
            // Move track to show current card at center
            testimonialTrack.style.transform = `translateX(calc(50% - ${(testimonialIndex * cardWidth) + (cardWidth / 2)}px))`;
            
            // Update active classes
            testimonialCards.forEach((card, index) => {
                card.classList.remove('active');
                if (index === testimonialIndex) {
                    card.classList.add('active');
                }
            });
        }
        
        function goToTestimonialSlide(index) {
            testimonialIndex = index;
            updateTestimonialCarousel();
            resetTestimonialAutoSlide();
        }
        
        function nextTestimonialSlide() {
            testimonialIndex = (testimonialIndex + 1) % testimonialTotal;
            updateTestimonialCarousel();
        }
        
        function prevTestimonialSlide() {
            testimonialIndex = (testimonialIndex - 1 + testimonialTotal) % testimonialTotal;
            updateTestimonialCarousel();
        }
        
        function startTestimonialAutoSlide() {
            if (!isHoveringTestimonial) {
                testimonialAutoSlide = setInterval(nextTestimonialSlide, 3000); // 3 seconds
            }
        }
        
        function resetTestimonialAutoSlide() {
            clearInterval(testimonialAutoSlide);
            startTestimonialAutoSlide();
        }
        
        // Event listeners for testimonials
        if (testimonialPrevBtn) {
            testimonialPrevBtn.addEventListener('click', () => {
                prevTestimonialSlide();
                resetTestimonialAutoSlide();
            });
        }
        
        if (testimonialNextBtn) {
            testimonialNextBtn.addEventListener('click', () => {
                nextTestimonialSlide();
                resetTestimonialAutoSlide();
            });
        }
        
        // ============================================
        // PAUSE ON HOVER - TESTIMONIAL CARDS
        // ============================================
        testimonialCards.forEach((card, index) => {
            card.addEventListener('mouseenter', function() {
                isHoveringTestimonial = true;
                clearInterval(testimonialAutoSlide);
                // Optional: Add visual feedback
                this.style.cursor = 'pointer';
            });
            
            card.addEventListener('mouseleave', function() {
                isHoveringTestimonial = false;
                // Add small delay before resuming
                setTimeout(() => {
                    if (!isHoveringTestimonial) {
                        startTestimonialAutoSlide();
                    }
                }, 100);
            });
        });
        
        // Pause on carousel container hover
        if (testimonialCarousel) {
            testimonialCarousel.addEventListener('mouseenter', () => {
                clearInterval(testimonialAutoSlide);
            });
            
            testimonialCarousel.addEventListener('mouseleave', () => {
                if (!isHoveringTestimonial) {
                    setTimeout(() => {
                        if (!isHoveringTestimonial) {
                            startTestimonialAutoSlide();
                        }
                    }, 100);
                }
            });
        }
        
        // ============================================
        // KEYBOARD NAVIGATION
        // ============================================
        document.addEventListener('keydown', (e) => {
            // Skip if user is typing in input or textarea
            if (document.activeElement.tagName === 'INPUT' || 
                document.activeElement.tagName === 'TEXTAREA') return;
            
            // Arrow keys for testimonials
            if (e.key === 'ArrowLeft') {
                prevTestimonialSlide();
                resetTestimonialAutoSlide();
            }
            if (e.key === 'ArrowRight') {
                nextTestimonialSlide();
                resetTestimonialAutoSlide();
            }
        });
        
        // ============================================
        // SWIPE SUPPORT FOR MOBILE
        // ============================================
        let startX = 0;
        let endX = 0;
        
        // Internship swipe
        if (internshipTrack) {
            internshipTrack.addEventListener('touchstart', (e) => {
                startX = e.touches[0].clientX;
            });
            
            internshipTrack.addEventListener('touchend', (e) => {
                endX = e.changedTouches[0].clientX;
                handleInternshipSwipe();
            });
        }
        
        function handleInternshipSwipe() {
            const threshold = 50;
            const difference = startX - endX;
            
            if (Math.abs(difference) > threshold) {
                if (difference > 0) {
                    nextInternshipSlide(); // Swipe left
                } else {
                    prevInternshipSlide(); // Swipe right
                }
                resetInternshipAutoSlide();
            }
        }
        
        // Testimonial swipe
        if (testimonialTrack) {
            testimonialTrack.addEventListener('touchstart', (e) => {
                startX = e.touches[0].clientX;
            });
            
            testimonialTrack.addEventListener('touchend', (e) => {
                endX = e.changedTouches[0].clientX;
                handleTestimonialSwipe();
            });
        }
        
        function handleTestimonialSwipe() {
            const threshold = 50;
            const difference = startX - endX;
            
            if (Math.abs(difference) > threshold) {
                if (difference > 0) {
                    nextTestimonialSlide(); // Swipe left
                } else {
                    prevTestimonialSlide(); // Swipe right
                }
                resetTestimonialAutoSlide();
            }
        }
        
        // ============================================
        // INITIALIZE CAROUSELS
        // ============================================
        if (internshipCards.length) {
            updateInternshipCarousel();
            startInternshipAutoSlide();
        }
        
        if (testimonialCards.length) {
            updateTestimonialCarousel();
            startTestimonialAutoSlide();
        }
        
        // ============================================
        // HANDLE WINDOW RESIZE
        // ============================================
        window.addEventListener('resize', () => {
            if (internshipCards.length) updateInternshipCarousel();
            if (testimonialCards.length) updateTestimonialCarousel();
        });
    });
</script>
</body>
</html>